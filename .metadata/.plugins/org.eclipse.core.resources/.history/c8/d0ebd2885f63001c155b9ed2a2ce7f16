package com.in28minutes.oops.level2;

public class Fan {
	private String make;
	private double radius;
	private String color;
	private boolean isOn;
	private byte speed; //0-5
	
	public Fan(String make, double radius, String color) {
		this.color=color;
		this.make=make;
		this.radius=radius;
		
		
	}
	
	public String toString() {
		return String.format("make - %s, radius - %d, color-%s, isOn-%b,speed-%d",this.make,this.radius,this.color,this.isOn,this.speed);
	}

}
